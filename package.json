{
    "name": "vscode-flatbuffers",
    "displayName": "FlatBuffers schema",
    "description": "Visual Studio Code extension for FlatBuffers schema files.",
    "publisher": "floxay",
    "author": {
        "name": "Huba Tuba"
    },
    "icon": "images/flatbuffers-logo-clean.png",
    "version": "0.1.1",
    "license": "MIT",
    "engines": {
        "vscode": "^1.40.0"
    },
    "homepage": "https://github.com/floxay/vscode-flatbuffers",
    "bugs": {
        "url": "https://github.com/floxay/vscode-flatbuffers/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/floxay/vscode-flatbuffers.git"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "flatbuffers"
    ],
    "contributes": {
        "languages": [
            {
                "id": "flatbuffers",
                "aliases": [
                    "Flatbuffers",
                    "FlatBuffers",
                    "Flatbuffers schema",
                    "FlatBuffers schema",
                    "fbs",
                    "idl"
                ],
                "extensions": [
                    ".fbs",
                    ".idl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "flatbuffers",
                "scopeName": "source.flatbuffers",
                "path": "./syntaxes/flatbuffers.tmLanguage.json"
            }
        ]
    }
}